{
	"definitions": {},
	"$schema": "http://json-schema.org/draft-07/schema#", 
	"$id": "https://example.com/object1648385000.json", 
	"title": "Root", 
	"type": "object",
	"required": [
		"users"
	],
	"properties": {
		"users": {
			"$id": "#root/users", 
			"title": "Users", 
			"type": "array",
			"default": [],
			"items":{
				"$id": "#root/users/items", 
				"title": "Items", 
				"type": "object",
				"required": [
					"id",
					"firstName",
					"lastName",
					"skillmap"
				],
				"properties": {
					"id": {
						"$id": "#root/users/items/id", 
						"title": "Id", 
						"type": "string",
						"default": "",
						"examples": [
							"U02"
						],
						"pattern": "^.*$"
					},
					"firstName": {
						"$id": "#root/users/items/firstName", 
						"title": "Firstname", 
						"type": "string",
						"default": "",
						"examples": [
							"Magha"
						],
						"pattern": "^.*$"
					},
					"lastName": {
						"$id": "#root/users/items/lastName", 
						"title": "Lastname", 
						"type": "string",
						"default": "",
						"examples": [
							"Patel"
						],
						"pattern": "^.*$"
					},
					"skillmap": {
						"$id": "#root/users/items/skillmap", 
						"title": "Skillmap", 
						"type": "array",
						"default": [],
						"items":{
							"$id": "#root/users/items/skillmap/items", 
							"title": "Items", 
							"type": "object",
							"required": [
								"id",
								"skill"
							],
							"properties": {
								"id": {
									"$id": "#root/users/items/skillmap/items/id", 
									"title": "Id", 
									"type": "integer",
									"examples": [
										2
									],
									"default": 0
								},
								"skill": {
									"$id": "#root/users/items/skillmap/items/skill", 
									"title": "Skill", 
									"type": "string",
									"default": "",
									"examples": [
										"Spring Boot"
									],
									"pattern": "^.*$"
								}
							}
						}

					}
				}
			}

		}
	}
}
